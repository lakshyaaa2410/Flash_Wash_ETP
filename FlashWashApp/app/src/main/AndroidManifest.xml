<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/new_logo_1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/new_logo_1"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat"
        tools:targetApi="31">
        <activity
            android:name=".Login"
            android:exported="false" />
        <activity
            android:name=".Client_booked_appointment"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ViewAppointments"
            android:exported="false"
            android:parentActivityName=".WasherActivity" />
        <activity
            android:name=".WasherBook"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".updateDetails"
            android:exported="false"
            android:parentActivityName=".WasherActivity" />
        <activity
            android:name=".WasherActivity"
            android:exported="false" />
        <activity
            android:name=".Splash_Activity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SignUp"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>